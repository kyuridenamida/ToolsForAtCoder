        <directoryProjectGenerator implementation="atcodertools.intellij.clion.wizard.AtCoderToolsProjectGenerator"/>
        <applicationService serviceInterface="atcodertools.intellij.common.service.AtCoderToolsApplicationService"
                            serviceImplementation="atcodertools.intellij.clion.service.AtCoderToolsApplicationServiceImpl" />

        <projectService serviceInterface="atcodertools.intellij.common.service.AtCoderToolsProjectService"
